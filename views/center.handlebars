                <div class="col-md-6">
                    <!-- create button -->
                    <button type="button" class="btn btn-default btn-lg btn-block" id="button-create" data-toggle='modal' data-target='#modal--create'>Create an Event</button>
                </div>
            </div>
            <!-- Modal for Create Feature -->
            <div class="modal fade" id="modal--create" tabindex="-1" role="dialog" aria-labelledby="modal--create">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-body">
                            <div class="panel panel-default">
                                <div class="panel-heading">
                                    <h3 class="panel-title">Enter Information on Your Event</h3>
                                </div>
                                <div class="panel-body">
                                    <form method="POST" action="/api/newGame" id="form--event-add">
                                        <div class="input-group">
                                            <label class="input-group-addon" for="course">Player Information</label>
                                            <input type="text" id="player-name" name="playername" class="form-control" placeholder="Player Name">
                                            <input type="text" id="player-email" name="email" class="form-control" placeholder="Email Address">
                                            <input type="text" id="player-password" name="password" class="form-control" placeholder="Password">
                                        </div>
                                        <div class="input-group">
                                            <label class="input-group-addon" for="player">Event Information</label>
                                            <input type="text" id="team-name" name="teamname" class="form-control" placeholder="Team Name">
                                            <input type="text" id="course-name" name="coursename" class="form-control" placeholder="Course Name">
                                            <input type="text" id="event-date" name="date" class="form-control" placeholder="Date">
                                            <input type="text" id="event-time" name="time" class="form-control" placeholder="Time">
                                        </div>
                                        <!-- Submit Button -->
                                        <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
                                        <button class="btn btn-default" method="GET" action="/" type="submit" id="form--event-add">Submit</button>
                                    </form>
                                </div>
                            </div>
                        </div>
